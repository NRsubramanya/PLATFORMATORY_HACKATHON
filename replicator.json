    "dest.kafka.sasl.jaas.config": "org.apache.kafka.common.security.plain.PlainLoginModule required username=\"6P6YFMXNYJ44K43N\" password=\"4xk+pRFLt3LMOAyLaFCIXMwe9Kj0c7Lu/wL/z4Zs9ss0mNcQH/n1ObXC4etRPTxS\";",
